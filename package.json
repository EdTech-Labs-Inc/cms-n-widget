{
  "name": "tmp",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "sst:dev": "sst dev",
    "sst:deploy": "sst deploy --stage staging",
    "sst:remove": "sst remove --stage staging"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "sst": "^3.17.23",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.6.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
